#include <iostream>
using namespace std;
int reversedigit(int n){
   int ans=0;
   int i=0;
   while(n!=0){
        int digit=n%10;
          ans=(ans*10)+digit;
          n=n/10;
   }
   return ans;
}
int main(){
          int n;
          cout<<"Enter the number ?"<<endl;
          cin>>n;
          cout<<"Before reversing the number is :"<<n<<endl;
          cout<<"After  reversing the number is :"<<reversedigit(n);
          
}
